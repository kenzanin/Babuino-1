{
  "name": "Blockly Babuino",
  "description": "A Babuino development environment based on Google Blockly.",
  "version": "0.1",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  
  "permissions": [
	"serial", 
	"storage",
	{"fileSystem": ["write"]},
    "unlimitedStorage",
    "contextMenus"
  ],
  
  "icons": {
    "16": "icon_16.png",
    "128": "babuino.png"
  },
  "app": {
    "background": {
      "scripts": ["launch.js"]
     }
  }
}
