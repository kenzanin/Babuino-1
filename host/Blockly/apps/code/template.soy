    <category name="{$ij.MSG.catProcedures}" custom="PROCEDURE"></category>
	<category name="{$ij.MSG.catMotor}">
	  <block type="babuino_motor_cmd"></block>
	  <block type="babuino_motor_setpower"></block>
      <block type="babuino_motor_onfor"></block>
	</category>
	<category name="{$ij.MSG.catIO}">
	  <block type="babuino_switch"></block>
	  <block type="babuino_sensor"></block>
